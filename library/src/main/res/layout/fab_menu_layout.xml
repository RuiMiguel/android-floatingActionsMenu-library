<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/floating_actions_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

  <RelativeLayout
      android:id="@+id/floating_actions_overlay"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:background="#7d000000"
      android:visibility="invisible"
      >
  </RelativeLayout>

  <RelativeLayout
      android:id="@+id/floating_actions_menu"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_alignParentStart="true"
      android:layout_alignParentTop="true"
      android:layout_marginBottom="@dimen/fab_margin_bottom"
      >

    <RelativeLayout
        android:id="@+id/floating_actions_submenu_horizontal"
        android:layout_width="match_parent"
        android:layout_height="@dimen/fab_height"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        >
      <View
          android:id="@+id/fake_menu_button_submenu_horizontal"
          android:layout_width="@dimen/fab_width"
          android:layout_height="@dimen/fab_height"
          android:layout_centerHorizontal="true"
          />
      <ImageView
          android:id="@+id/floating_action_button1"
          android:layout_width="@dimen/fab_width_mini"
          android:layout_height="@dimen/fab_height_mini"
          android:layout_centerVertical="true"
          android:layout_marginRight="29dp"
          android:layout_toLeftOf="@id/fake_menu_button_submenu_horizontal"
          android:src="@android:drawable/ic_menu_call"
          />
      <ImageView
          android:id="@+id/floating_action_button2"
          android:layout_width="@dimen/fab_width_mini"
          android:layout_height="@dimen/fab_height_mini"
          android:layout_centerVertical="true"
          android:layout_marginLeft="29dp"
          android:layout_toRightOf="@id/fake_menu_button_submenu_horizontal"
          android:src="@android:drawable/ic_menu_camera"
          />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/floating_actions_submenu_fan"
        android:layout_width="450dp"
        android:layout_height="200dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        >
      <View
          android:id="@+id/fake_menu_button_submenu_fan"
          android:layout_width="@dimen/fab_width_mini"
          android:layout_height="@dimen/fab_height"
          android:layout_alignParentBottom="true"
          android:layout_centerHorizontal="true"
          />
      <ImageView
          android:id="@+id/floating_action_button3"
          android:layout_width="@dimen/fab_width"
          android:layout_height="@dimen/fab_height"
          android:layout_alignParentBottom="true"
          android:layout_marginBottom="48dp"
          android:layout_marginRight="50dp"
          android:layout_toLeftOf="@+id/fake_menu_button_submenu_fan"
          android:src="@android:drawable/ic_menu_today"
          />
      <ImageView
          android:id="@+id/floating_action_button4"
          android:layout_width="@dimen/fab_width"
          android:layout_height="@dimen/fab_height"
          android:layout_above="@id/floating_action_button3"
          android:layout_marginBottom="13dp"
          android:layout_toLeftOf="@id/fake_menu_button_submenu_fan"
          android:src="@android:drawable/ic_menu_compass"
          />
      <ImageView
          android:id="@+id/floating_action_button5"
          android:layout_width="@dimen/fab_width"
          android:layout_height="@dimen/fab_height"
          android:layout_alignParentBottom="true"
          android:layout_marginBottom="48dp"
          android:layout_marginLeft="50dp"
          android:layout_toRightOf="@+id/fake_menu_button_submenu_fan"
          android:src="@android:drawable/ic_menu_zoom"
          />
      <ImageView
          android:id="@+id/floating_action_button6"
          android:layout_width="@dimen/fab_width"
          android:layout_height="@dimen/fab_height"
          android:layout_above="@id/floating_action_button5"
          android:layout_marginBottom="13dp"
          android:layout_toRightOf="@id/fake_menu_button_submenu_fan"
          android:src="@android:drawable/ic_menu_agenda"
          />
    </RelativeLayout>

    <ImageView
        android:id="@+id/floating_action_menu_button"
        android:layout_width="@dimen/fab_width"
        android:layout_height="@dimen/fab_height"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:src="@android:drawable/ic_dialog_map"
        />
  </RelativeLayout>
</RelativeLayout>
