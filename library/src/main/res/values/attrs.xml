<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="FloatingActionsMenu">
    <attr format="enum"
        name="fab_vertical_alignment">
      <enum name="center"
          value="0"/>
      <enum name="top"
          value="1"/>
      <enum name="bottom"
          value="2"/>
    </attr>
    <attr format="enum" name="fab_horizontal_alignment">
      <enum name="center"
          value="0"/>
      <enum name="left"
          value="1"/>
      <enum name="right"
          value="2"/>
    </attr>
    <attr format="enum"
        name="fab_menu_button_size">
      <enum name="normal"
          value="0"/>
      <enum name="mini"
          value="1"/>
    </attr>
    <attr format="color"
        name="fab_bg_menu_button_color_normal"/>
    <attr format="color"
        name="fab_bg_menu_button_color_pressed"/>
    <attr format="reference"
        name="fab_menu_icon"/>

    <attr format="boolean"
        name="fab_addButtonStrokeVisible"/>
  </declare-styleable>

  <declare-styleable name="FloatingActionsSubmenu">
    <attr format="enum"
        name="fab_expand_direction">
      <enum name="up"
          value="0"/>
      <enum name="down"
          value="1"/>
      <enum name="left"
          value="2"/>
      <enum name="right"
          value="3"/>
      <enum name="round"
          value="4"/>
      <enum name="fan"
          value="5"/>
    </attr>
    <attr format="integer"
        name="fab_button_spacing"/>
    <attr format="dimension"
        name="fab_submenu_radius"/>
    <attr format="integer"
        name="fab_submenu_angle_overflow"/>
    <attr format="boolean"
        name="fab_enable_overlay"/>
    <attr format="reference"
        name="fab_submenu_icon"/>
    <attr format="string"
        name="fab_submenu_group"/>
  </declare-styleable>

  <declare-styleable name="FloatingActionMenuButton">
  </declare-styleable>

  <declare-styleable name="FloatingActionButton">
    <attr format="color"
        name="fab_bg_color_normal"/>
    <attr format="color"
        name="fab_bg_color_pressed"/>
    <attr format="color"
        name="fab_bg_color_disabled"/>

    <attr format="reference"
        name="fab_icon"/>
    <attr format="enum"
        name="fab_size">
      <enum name="normal"
          value="0"/>
      <enum name="mini"
          value="1"/>
    </attr>

    <attr format="boolean"
        name="fab_stroke_visible"/>
  </declare-styleable>
</resources>